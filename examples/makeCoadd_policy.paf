#<? cfg paf policy ?>

# save intermediate images?
saveDebugImages: false

# pixels with these mask plane bits other than these are omitted from the coadd
allowedMaskPlanes: "BAD SAT INTRP"

# doWarpExposures: # warp exposures to match the reference exposure?
# if false then all exposures are assumed to match the reference in WCS and size
doWarpExposures: true

# name of warping kernel, e.g. lanczos4 or bilinear
warpingKernelName: lanczos4
